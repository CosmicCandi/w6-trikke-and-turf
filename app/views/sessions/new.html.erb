
<div class="container">
  <h1>Login:</h1>
  <div class="row">
    <div class="col-sm-6 col-sm-offset-1">
      <%= simple_form_for :session do |f| %>
      <%= f.input :username %>
      <%= f.input :password %>
      <%= f.button :submit, "Login", class: "btn btn-primary" %>
      <% end %>
    </div>
  </div>
</div>
